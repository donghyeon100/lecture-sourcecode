<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.kh.test.model.mapper.StudentMapper">

	<select id="studentSelect" resultType="Student">
		SELECT * FROM STUDENT
		ORDER BY STUDENT_NO
	</select>


	<insert id="studentInsert">
		INSERT INTO STUDENT VALUES(SEQ_STUDENT_NO.NEXTVAL, #{studentName}, #{studentMajor}, #{studentGender})
	</insert>
	
	<delete id="studentDelete">
		DELETE FROM STUDENT WHERE STUDENT_NO = #{studentNo}
	</delete>
</mapper>

